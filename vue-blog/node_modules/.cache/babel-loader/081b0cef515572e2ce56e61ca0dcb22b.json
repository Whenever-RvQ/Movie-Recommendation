       </div>\r\n        <!-- 主内容 -->\r\n        <div class=\"welcome-container\">\r\n            \r\n            <!-- 个人标识（可替换为头像） -->\r\n            <div class=\"personal-mark\">\r\n                <img src=\"@/assets/img/avatar.jpg\">\r\n            </div>\r\n\r\n            <!-- 欢迎语 -->\r\n            <h1 class=\"greeting\">Hello, I'm <span>Whenever_RvQ</span></h1>\r\n            <p class=\"intro\">\r\n                热爱生活、追求极致。这里是我的个人空间，分享我的作品、想法与成长历程。\r\n            </p>\r\n\r\n            <!-- 行动按钮 -->\r\n            <div class=\"action-buttons\">\r\n                <a href=\"#about\" class=\"btn btn-primary\">\r\n                    <i class=\"fas fa-user-circle\"></i>关于我\r\n                </a>\r\n                <a href=\"#works\" class=\"btn btn-secondary\">\r\n                    <i class=\"fas fa-briefcase\"></i>我的作品\r\n                </a>\r\n            </div>\r\n\r\n            <!-- 社交链接 -->\r\n            <div class=\"social-links\">\r\n                <a href=\"https://github.com/Whenever-RvQ\" aria-label=\"GitHub\"><i class=\"fab fa-github\"></i></a>\r\n                <a href=\"#\" aria-label=\"LinkedIn\"><i class=\"fab fa-qq\"></i></a>\r\n                <a href=\"#\" aria-label=\"微博\"><i class=\"fab fa-weixin\"></i></a>\r\n                <a href=\"#\" aria-label=\"邮箱\"><i class=\"fas fa-envelope\"></i></a>\r\n            </div>\r\n\r\n            <!-- 版权信息 -->\r\n            <div class=\"copyright\">© 2025 Whevener_RvQ. All rights reserved.</div>\r\n        </div>\r\n    </el-container>\r\n</template>\r\n\r\n<script>\r\n\r\nimport MINXIN from '@/core/minxin'\r\nexport default {\r\n    name: 'Welcome',\r\n    mixins: [MINXIN],\r\n    components: {\r\n\r\n    },\r\n    data() {\r\n        return {\r\n\r\n        }\r\n    },\r\n    methods: {\r\n\r\n    }\r\n}\r\n</script>\r\n<style lang=\"stylus\">\r\n\r\n* {\r\n      margin: 0;\r\n      padding: 0;\r\n      box-sizing: border-box;\r\n    }\r\n\r\n    /* 全局变量 */\r\n    :root {\r\n      --primary: #2d7ff9;\r\n      --accent: #6c5ce7;\r\n      --neutral: #f8f9fa;\r\n      --text-dark: #2d3436;\r\n      --text-light: #636e72;\r\n      --shadow: 0 8px 32px rgba(0, 0, 0, 0.06);\r\n      --transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\r\n      --bg-gradient: linear-gradient(120deg, #fafafa 0%, #f5f6f7 100%);\r\n    }\r\n\r\n    /* 全局样式 */\r\n    // body {\r\n    //   font-family: 'Inter', 'Segoe UI', system-ui, sans-serif;\r\n    //   background: var(--bg-gradient);\r\n    //   min-height: 100vh;\r\n    //   display: flex;\r\n    //   align-items: center;\r\n    //   justify-content: center;\r\n    //   overflow-x: hidden;\r\n    //   position: relative;\r\n    // }\r\n\r\n    /* 背景动态装饰 */\r\n    .bg-dot {\r\n      position: absolute;\r\n      border-radius: 50%;\r\n      background: rgba(45, 127, 249, 0.05);\r\n      z-index: -1;\r\n      animation: float 12s infinite ease-in-out;\r\n    }\r\n\r\n    .bg-dot-1 {\r\n      width: 400px;\r\n      height: 400px;\r\n      top: -150px;\r\n      right: -150px;\r\n      animation-delay: 0s;\r\n    }\r\n\r\n    .bg-dot-2 {\r\n      width: 300px;\r\n      height: 300px;\r\n      bottom: -100px;\r\n      left: -100px;\r\n      animation-delay: 3s;\r\n    }\r\n\r\n    .bg-dot-3 {\r\n      width: 200px;\r\n      height: 200px;\r\n      top: 50%;\r\n      left: 10%;\r\n      transform: translateY(-50%);\r\n      animation-delay: 6s;\r\n    }\r\n\r\n    /* 主内容容器 */\r\n    .welcome-container {\r\n      text-align: center;\r\n      max-width: 700px;\r\n      width: 100%;\r\n      padding: 60px 25px;\r\n      position: relative;\r\n    }\r\n\r\n    /* 个人标识（可选头像/图标） */\r\n    .personal-mark {\r\n      width: 100px;\r\n      height: 100px;\r\n      margin: 0 auto 40px;\r\n      border-radius: 50%;\r\n      background: linear-gradient(135deg, var(--primary), var(--accent));\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      color: white;\r\n      font-size: 2.5rem;\r\n      box-shadow: 0 10px 25px rgba(45, 127, 249, 0.2);\r\n      transition: var(--transition);\r\n      overflow: hidden;\r\n    }\r\n\r\n    .personal-mark:hover {\r\n      transform: scale(1.08) rotate(5deg);\r\n      box-shadow: 0 15px 30px rgba(45, 127, 249, 0.3);\r\n    }\r\n\r\n    .personal-mark>img{\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n\r\n    /* 文字样式 */\r\n    .greeting {\r\n      font-size: 3.2rem;\r\n      font-weight: 700;\r\n      color: var(--text-dark);\r\n      margin-bottom: 15px;\r\n      line-height: 1.2;\r\n    }\r\n\r\n    .greeting span {\r\n      background: linear-gradient(90deg, var(--primary), var(--accent));\r\n      -webkit-background-clip: text;\r\n      background-clip: text;\r\n      color: transparent;\r\n    }\r\n\r\n    .intro {\r\n      font-size: 1.2rem;\r\n      color: var(--text-light);\r\n      margin-bottom: 50px;\r\n      line-height: 1.8;\r\n      max-width: 550px;\r\n      margin-left: auto;\r\n      margin-right: auto;\r\n    }\r\n\r\n    /* 行动按钮 */\r\n    .action-buttons {\r\n      display: flex;\r\n      flex-wrap: wrap;\r\n      justify-content: center;\r\n      gap: 15px;\r\n      margin-bottom: 60px;\r\n    }\r\n\r\n    .btn {\r\n      padding: 14px 32px;\r\n      border-radius: 8px;\r\n      font-weight: 600;\r\n      font-size: 1rem;\r\n      text-decoration: none;\r\n      transition: var(--transition);\r\n      border: none;\r\n      cursor: pointer;\r\n      display: inline-flex;\r\n      align-items: center;\r\n      gap: 8px;\r\n    }\r\n\r\n    .btn-primary {\r\n      background: linear-gradient(90deg, var(--primary), var(--accent));\r\n      color: white;\r\n      box-shadow: 0 4px 15px rgba(45, 127, 249, 0.25);\r\n    }\r\n\r\n    .btn-primary:hover {\r\n      transform: translateY(-3px);\r\n      box-shadow: 0 8px 20px rgba(45, 127, 249, 0.35);\r\n      background: linear-gradient(90deg, #2170f9, #5d4ce7);\r\n    }\r\n\r\n    .btn-secondary {\r\n      background: white;\r\n      color: var(--primary);\r\n      border: 1px solid rgba(45, 127, 249, 0.2);\r\n    }\r\n\r\n    .btn-secondary:hover {\r\n      background: var(--neutral);\r\n      border-color: var(--primary);\r\n      transform: translateY(-3px);\r\n    }\r\n\r\n    /* 社交链接 */\r\n    .social-links {\r\n      display: flex;\r\n      justify-content: center;\r\n      gap: 20px;\r\n    }\r\n\r\n    .social-links a {\r\n      width: 48px;\r\n      height: 48px;\r\n      border-radius: 8px;\r\n      background: white;\r\n      color: var(--text-light);\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      font-size: 1.2rem;\r\n      transition: var(--transition);\r\n      box-shadow: var(--shadow);\r\n    }\r\n\r\n    .social-links a:hover {\r\n      color: var(--primary);\r\n      transform: translateY(-5px);\r\n      box-shadow: 0 8px 15px rgba(45, 127, 249, 0.15);\r\n    }\r\n\r\n    /* 底部版权 */\r\n    .copyright {\r\n      display: block;\r\n      position: absolute;\r\n      margin: 0 auto\r\n      bottom: 25px;\r\n      left: 30%;\r\n      color: var(--text-light);\r\n      font-size: 0.9rem;\r\n      opacity: 0.7;\r\n    }\r\n\r\n    /* 动画效果 */\r\n    @keyframes float {\r\n      0% { transform: translate(0, 0) rotate(0deg); }\r\n      50% { transform: translate(15px, -15px) rotate(5deg); }\r\n      100% { transform: translate(0, 0) rotate(0deg); }\r\n    }\r\n\r\n    /* 页面加载动画 */\r\n    @keyframes fadeIn {\r\n      from { opacity: 0; transform: translateY(20px); }\r\n      to { opacity: 1; transform: translateY(0); }\r\n    }\r\n\r\n    .welcome-container > * {\r\n      animation: fadeIn 0.8s ease forwards;\r\n      opacity: 0;\r\n    }\r\n\r\n    .personal-mark { animation-delay: 0.2s; }\r\n    .greeting { animation-delay: 0.4s; }\r\n    .intro { animation-delay: 0.6s; }\r\n    .action-buttons { animation-delay: 0.8s; }\r\n    .social-links { animation-delay: 1s; }\r\n\r\n    /* 响应式设计 */\r\n    @media (max-width: 768px) {\r\n      .greeting {\r\n        font-size: 2.5rem;\r\n      }\r\n\r\n      .intro {\r\n        font-size: 1.1rem;\r\n      }\r\n\r\n      .btn {\r\n        padding: 12px 28px;\r\n        font-size: 0.95rem;\r\n      }\r\n\r\n      .social-links {\r\n        gap: 15px;\r\n      }\r\n\r\n      .social-links a {\r\n        width: 42px;\r\n        height: 42px;\r\n        font-size: 1.1rem;\r\n      }\r\n    }\r\n\r\n    @media (max-width: 480px) {\r\n      .welcome-container {\r\n        padding: 40px 20px;\r\n      }\r\n\r\n      .greeting {\r\n        font-size: 2rem;\r\n      }\r\n\r\n      .intro {\r\n        font-size: 1rem;\r\n        margin-bottom: 40px;\r\n      }\r\n\r\n      .action-buttons {\r\n        flex-direction: column;\r\n        gap: 12px;\r\n      }\r\n\r\n      .btn {\r\n        width: 100%;\r\n        justify-content: center;\r\n      }\r\n\r\n      .bg-dot-1 {\r\n        width: 300px;\r\n        height: 300px;\r\n        top: -100px;\r\n        right: -100px;\r\n      }\r\n\r\n      .bg-dot-2 {\r\n        width: 200px;\r\n        height: 200px;\r\n        bottom: -80px;\r\n        left: -80px;\r\n      }\r\n      \r\n    }\r\n\r\n     /* 登录注册按钮容器（控制布局） */\r\n    .auth-buttons {\r\n      position: absolute;\r\n      right: 0px;\r\n      top: 0px;\r\n      display: flex;\r\n      flex-wrap: wrap;\r\n      justify-content: center;\r\n      gap: 18px;\r\n      margin: 20px 0px;\r\n      scale: 0.8\r\n    }\r\n\r\n    /* 核心按钮样式（与之前行动按钮风格一致，新增专属动画） */\r\n    .auth-btn {\r\n      height: auto;\r\n      padding: 16px 36px;\r\n      border-radius: 12px;\r\n      font-weight: 600;\r\n      font-size: 1.1rem;\r\n      text-decoration: none;\r\n      transition: var(--transition);\r\n      border: none;\r\n      cursor: pointer;\r\n      display: inline-flex;\r\n      align-items: center;\r\n      gap: 10px;\r\n      overflow: hidden;\r\n    }\r\n\r\n    /* 登录按钮（主色渐变，带发光动画） */\r\n    .auth-btn-login {\r\n      background: linear-gradient(90deg, var(--primary), var(--accent));\r\n      color: white;\r\n      box-shadow: 0 6px 20px rgba(45, 127, 249, 0.25);\r\n    }\r\n\r\n    .auth-btn-login:hover {\r\n      transform: translateY(-4px);\r\n      box-shadow: 0 10px 25px rgba(45, 127, 249, 0.35);\r\n      background: linear-gradient(90deg, #2170f9, #5d4ce7);\r\n    }\r\n\r\n    /* 登录按钮发光动画（hover时触发） */\r\n    .auth-btn-login::after {\r\n      content: '';\r\n      position: absolute;\r\n      top: 50%;\r\n      left: 50%;\r\n      width: 140%;\r\n      height: 140%;\r\n      background: rgba(255, 255, 255, 0.15);\r\n      border-radius: 50%;\r\n      transform: translate(-50%, -50%) scale(0);\r\n      transition: transform 0.6s ease;\r\n    }\r\n\r\n    .auth-btn-login:hover::after {\r\n      transform: translate(-50%, -50%) scale(1);\r\n      opacity: 0;\r\n    }\r\n\r\n    /* 注册按钮（白底描边，带填充动画） */\r\n    .auth-btn-register {\r\n      background: white;\r\n      color: var(--primary);\r\n      border: 2px solid rgba(45, 127, 249, 0.3);\r\n      box-shadow: var(--shadow);\r\n    }\r\n\r\n    .auth-btn-register:hover {\r\n      transform: translateY(-4px);\r\n      border-color: var(--primary);\r\n      box-shadow: 0 10px 25px rgba(45, 127, 249, 0.15);\r\n    }\r\n\r\n    /* 注册按钮填充动画（hover时背景渐变填充） */\r\n    .auth-btn-register::before {\r\n      content: '';\r\n      position: absolute;\r\n      top: 0;\r\n      left: 0;\r\n      width: 0;\r\n      height: 100%;\r\n      background: linear-gradient(90deg, rgba(45, 127, 249, 0.05), rgba(108, 92, 231, 0.05));\r\n      transition: width 0.5s ease;\r\n      z-index: -1;\r\n    }\r\n\r\n    .auth-btn-register:hover::before {\r\n      width: 100%;\r\n    }\r\n\r\n    /* 图标样式（确保与文字对齐） */\r\n    .auth-btn i {\r\n      font-size: 1.2rem;\r\n      transition: transform 0.3s ease;\r\n    }\r\n\r\n    .auth-btn:hover i {\r\n      transform: translateX(3px);\r\n    }\r\n\r\n    /* 响应式适配（与之前页面保持一致） */\r\n    @media (max-width: 768px) {\r\n      .auth-btn {\r\n        padding: 14px 30px;\r\n        font-size: 1rem;\r\n      }\r\n    }\r\n\r\n    @media (max-width: 480px) {\r\n      .auth-buttons {\r\n        flex-direction: column;\r\n        gap: 15px;\r\n        width: 100%;\r\n      }\r\n\r\n      .auth-btn {\r\n        width: 100%;\r\n        justify-content: center;\r\n        padding: 14px 20px;\r\n      }\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/base"}]}