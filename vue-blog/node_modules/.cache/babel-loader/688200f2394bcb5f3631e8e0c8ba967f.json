ed')\r\n  },\r\n  computed: {\r\n    like_num() {\r\n      localStorage.setItem('like_num', this.article.like_num)\r\n      return localStorage.getItem('like_num')\r\n    }\r\n  },\r\n  methods: {\r\n    reload() {\r\n      this.isRouteLoading = false\r\n      this.$nextTick(() => {\r\n        this.isRouteLoading = true\r\n        this.getArticleById()\r\n      });\r\n    },\r\n    getArticleById() {\r\n      this.$api({ type: 'getArticleById', data: { id: this.id } }).then(result => {\r\n        this.article = result\r\n      }).catch(err => {\r\n        this.$notify.success({\r\n          title: '获取失败',\r\n          message: err.message\r\n        })\r\n      })\r\n    },\r\n  },\r\n\r\n  getCurrentPath() {\r\n    return this.$route.path\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\n.blog-main--card\r\n  width 100%\r\n\r\n</style>"],"sourceRoot":"src/views"}]}